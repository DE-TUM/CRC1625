SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?user ?name ?ML ?ML_label ?ML_comment WHERE {
      ?ML a/rdfs:subClassOf* crc:MaterialsLibrary.
      # Get their label and comment
      ?ML crc:objectName ?ML_label.
      OPTIONAL { ?ML crc:objectDescription ?ML_comment. } # Make it optional. Some have no comment at all

      ?ML crc:assignedTo ?user .
      ?user crc:name ?name.
    }
}