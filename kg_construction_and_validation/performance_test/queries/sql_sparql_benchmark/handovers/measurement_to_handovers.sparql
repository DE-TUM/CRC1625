SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?sample ?measurement ?handover
    WHERE {
        ?workflow_instance pmdco:input ?sample.
        ?workflow_instance pmdco:subordinateProcess ?first_handover_group.
        ?first_handover_group pmdco:nextProcess* ?handover_group. # All handover groups
        ?handover_group pmdco:subordinateProcess ?first_handover_of_group.
        ?first_handover_of_group pmdco:nextProcess* ?handover. # All handovers

        ?handover pmdco:subordinateProcess ?activity.
        ?activity pmdco:output ?measurement.

        ?sample pmdco:characteristic ?measurement.

        ?measurement a pmdco:ValueObject.
    }
}
