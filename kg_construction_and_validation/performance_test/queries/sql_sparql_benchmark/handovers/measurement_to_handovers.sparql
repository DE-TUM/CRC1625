SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?sample ?measurement ?handover
    WHERE {
        ?workflow_instance crc:input ?sample.
        ?workflow_instance crc:substep ?first_handover_group.
        ?first_handover_group crc:nextStep* ?handover_group. # All handover groups
        ?handover_group crc:substep ?first_handover_of_group.
        ?first_handover_of_group crc:nextStep* ?handover. # All handovers

        ?handover crc:substep ?activity.
        ?activity crc:output ?measurement.

        ?sample crc:characteristic ?measurement.

        ?measurement a pmdco:ValueObject.
    }
}
