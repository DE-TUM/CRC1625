SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?ML ?externalID ?internalID ?creationDate ?user ?substrate_label
    WHERE
    {
      ?ML a/rdfs:subClassOf* crc:MaterialsLibrary.

      ?ML crc:internalID ?internalID.
      ?ML crc:creationDate ?creationDate.
      ?ML crc:assignedTo ?user.

      ?ML crc:characteristic ?substrate.
      ?substrate a crc:Substrate.
      ?substrate crc:objectName ?substrate_label.
    }
}