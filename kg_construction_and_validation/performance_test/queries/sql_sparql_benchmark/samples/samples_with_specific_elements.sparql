SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?ML WHERE {
      ?ML a/rdfs:subClassOf* :MaterialsLibrary.
      ?ML pmdco:relatesToElement ?element_1, ?element_2.

      ?element_1 chebi:formula "Ag".
      ?element_2 chebi:formula "Au".

      # Note that we also allow samples with other elements among them
    }
}