SELECT (COUNT(*) AS ?benchmark_count)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
WHERE {
    SELECT ?user ?user_name ?measurement ?measurementLabel
    WHERE {
      # Look for exclusively EDX CSV measurements
      ?measurement a measurement_type:EDX_CSV.
      # Get their textual label and comment
      ?measurement rdfs:label ?measurementLabel.

      ?measurement prov:wasAttributedTo ?user.
      ?user foaf:name ?user_name.
    }
}