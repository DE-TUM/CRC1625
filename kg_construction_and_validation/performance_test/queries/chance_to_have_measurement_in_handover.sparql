SELECT
  (COUNT(?handover_w_measurement) as ?count_w)
  (COUNT(?handover) as ?total_handovers)
  (xsd:decimal(COUNT(?handover_w_measurement)) / xsd:decimal(COUNT(?handover)) as ?chance_to_have_measurement_in_handover)
FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph>
{
    {
        SELECT ?handover_w_measurement WHERE {
            ?handover_w_measurement a crc:Handover.

            ?handover_w_measurement crc:substep ?activity.
        }
    }

    UNION

    {
        SELECT ?handover WHERE {
            ?handover a crc:Handover.
        }
    }
}