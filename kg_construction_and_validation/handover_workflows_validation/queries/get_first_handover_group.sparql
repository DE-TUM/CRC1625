SELECT ?first_handover_group WHERE {
	GRAPH <https://crc1625.mdi.ruhr-uni-bochum.de/graph> {
		?workflow_instance a crc:HandoverWorkflowInstance.
		?MLOrSample a/rdfs:subClassOf* crc:MaterialsLibrary.

		?workflow_instance crc:input ?MLOrSample.
		?MLOrSample crc:internalID {object_id}.

		?workflow_instance crc:substep ?first_handover_group.
	}
}