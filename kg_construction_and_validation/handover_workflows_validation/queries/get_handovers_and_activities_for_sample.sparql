SELECT ?s ?p ?o FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph> WHERE {
    {
        ?MLOrSample crc:internalID {object_id}.

        ?workflow_instance crc:input ?MLOrSample.

        ?workflow_instance crc:substep/crc:nextStep* ?s. # Handover group

        ?s ?p ?o.
    }

    UNION

    {
        ?MLOrSample crc:internalID {object_id}.

        ?workflow_instance crc:input ?MLOrSample.

        ?workflow_instance crc:substep/crc:nextStep*/crc:substep/crc:nextStep* ?s. # Handover

        ?s ?p ?o.
    }

    UNION

    {
        ?MLOrSample crc:internalID {object_id}.

        ?workflow_instance crc:input ?MLOrSample.

        ?workflow_instance crc:substep/crc:nextStep*/crc:substep/crc:nextStep*/crc:substep ?s. # Activity

        ?s rdf:type ?o.

        BIND(rdf:type AS ?p)
    }
}