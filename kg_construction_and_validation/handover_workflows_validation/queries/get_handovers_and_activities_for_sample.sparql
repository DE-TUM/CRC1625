SELECT ?s ?p ?o FROM <https://crc1625.mdi.ruhr-uni-bochum.de/graph> WHERE {
    {
        ?MLOrSample :internalID {object_id}.

        ?workflow_instance pmdco:input ?MLOrSample.

        ?workflow_instance pmdco:subordinateProcess/pmdco:nextProcess* ?s. # Handover group

        ?s ?p ?o.
    }

    UNION

    {
        ?MLOrSample :internalID {object_id}.

        ?workflow_instance pmdco:input ?MLOrSample.

        ?workflow_instance pmdco:subordinateProcess/pmdco:nextProcess*/pmdco:subordinateProcess/pmdco:nextProcess* ?s. # Handover

        ?s ?p ?o.
    }

    UNION

    {
        ?MLOrSample :internalID {object_id}.

        ?workflow_instance pmdco:input ?MLOrSample.

        ?workflow_instance pmdco:subordinateProcess/pmdco:nextProcess*/pmdco:subordinateProcess/pmdco:nextProcess*/pmdco:subordinateProcess ?s. # Activity

        ?s rdf:type ?o.

        BIND(rdf:type AS ?p)
    }
}