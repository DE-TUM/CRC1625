SELECT ?workflow_model_name WHERE {
    {
        GRAPH <https://crc1625.mdi.ruhr-uni-bochum.de/graph/workflows> {
            ?s a crc:HandoverWorkflowModel.

            ?s crc:objectName ?workflow_model_name.

            ?s crc:creator ?user.
        }

        GRAPH <https://crc1625.mdi.ruhr-uni-bochum.de/graph> {
            ?user crc:internalID {user_id}.
        }
    }
}