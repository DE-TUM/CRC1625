SELECT ?type WHERE {
    GRAPH <https://crc1625.mdi.ruhr-uni-bochum.de/graph/workflows> {
        <{entity_iri}> rdf:type ?type.
    }
    GRAPH <https://crc1625.mdi.ruhr-uni-bochum.de/graph> {
        ?type rdfs:subClassOf ?parent_class.
        VALUES ?parent_class { pmdco:AnalysingProcess pmdco:MicroscopyProcess }.
    }
}